{
  "name": "github-achievement-generator",
  "version": "1.0.0",
  "description": "Automated tool to help unlock GitHub achievement badges",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "generate:pull-shark": "node dist/generators/pullShark.js",
    "generate:galaxy-brain": "node dist/generators/galaxyBrain.js",
    "generate:arctic-vault": "node dist/generators/arcticVault.js",
    "generate:starstruck": "node dist/generators/starstruck.js",
    "generate:all": "npm run generate:pull-shark && npm run generate:galaxy-brain && npm run generate:arctic-vault",
    "check-progress": "node dist/utils/checkProgress.js",
    "setup-tokens": "node dist/setup/tokens.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts"
  },
  "keywords": ["github", "achievements", "badges", "automation", "pull-shark", "galaxy-brain"],
  "author": "Dlanang",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "@octokit/graphql": "^7.0.2",
    "@octokit/auth-token": "^4.0.0",
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.2",
    "axios": "^1.5.0",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^21.3.8",
    "chalk": "^4.1.2",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1",
    "boxen": "^5.1.2",
    "figlet": "^1.6.0",
    "gradient-string": "^2.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.7",
    "@types/inquirer": "^9.0.3",
    "@types/figlet": "^1.5.6",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "typescript": "^5.1.6"
  }
}